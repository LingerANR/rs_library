<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_matricula" model="ir.sequence">
            <field name="name">Student Number</field>
            <field name="code">res.partner</field>
            <field name="prefix">POLI%(year)s%(woy)s%(month)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_loans" model="ir.sequence">
            <field name="name">Loan Count</field>
            <field name="code">book.loan</field>
            <field name="prefix">LOAN</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
